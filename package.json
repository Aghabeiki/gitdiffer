{
  "name": "gitdiffer",
  "displayName": "gitDiffer",
  "description": "Just lunch git difftools",
  "version": "0.0.1",
  "publisher": "aaghabeiki",
  "engines": {
    "vscode": "^1.11.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.gitDiffer"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.gitDiffer",
        "title": "lunch difftools on this file..."
      }
    ],
    "menus": {
      "scm/resourceState/context": [
        {
          "command": "extension.gitDiffer"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "simple-git": "^1.70.0"
  }
}
